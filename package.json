{
  "name": "akamai-sandbox-cli",
  "version": "1.8.0",
  "description": "A tool that makes it easier to manage Akamai Sandboxes. Call the Sandbox API from the command line.",
  "repository": "https://github.com/akamai/cli-sandbox",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run build-ts && npm run copy-json",
    "clean": "rm -rf ./bin",
    "build-ts": "tsc",
    "copy-json": "cp -r schemas src/template package.json ./bin",
    "postinstall": "npm run build"
  },
  "bin": {
    "sandbox": "./akamai-sandbox.js"
  },
  "engines": {
    "node": ">=20 <=24",
    "npm": ">=9 <=11"
  },
  "dependencies": {
    "akamai-edgegrid": "3.5.3",
    "app-root-path": "3.1.0",
    "cli-spinner": "0.2.10",
    "commander": "4.1.1",
    "decompress": "4.2.1",
    "easy-table": "1.2.0",
    "find-java-home": "2.0.0",
    "fs-extra": "11.3.0",
    "got": "^14.4.7",
    "inquirer": "12.5.2",
    "jsonschema": "1.5.0",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "shelljs": "0.9.2",
    "untildify": "5.0.0",
    "uuid": "11.1.0",
    "validator": "13.15.0"
  },
  "devDependencies": {
    "@types/lodash": "4.17.16",
    "@types/node": "22.14.0",
    "@types/request": "2.48.12",
    "typescript": "5.8.3"
  }
}
